    <Task>
      <Using Namespace="System.Diagnostics" />
      <Code Type="Fragment" Language="cs">
<![CDATA[
  try {
    Process p = new Process();
    p.StartInfo.UseShellExecute = false;
    p.StartInfo.RedirectStandardOutput = true;
    p.StartInfo.FileName = "aocl";
    p.StartInfo.Arguments = Command;
    try {
      p.Start();
    }
    catch(System.ComponentModel.Win32Exception ex) {
      if(ex.ErrorCode == -2147467259) {
        Log.LogMessage("ERROR: Could not find the 'aocl' command when trying to execute 'aocl " + Command + "'.\n" +
          "Please ensure that '%ALTERAOCLSDKROOT%\\bin' is on the path; " +
          "refer to the Altera SDK for OpenCL Getting Started Guide for more information on setting up your environment.",
          MessageImportance.High);
        return false;
      }
      throw ex;
    }
    Output = p.StandardOutput.ReadLine();
    p.WaitForExit();
  }
  catch(Exception ex) {
    Log.LogMessage("ERROR: Encountered exception while trying to execute 'aocl " + Command + "'.\n" +
      "Please check your environment set up; refer to the Altera SDK for OpenCL Getting Started Guide for more information.\n" +
      "Exception: " + ex, MessageImportance.High);
    return false;
  }
]]>
      </Code>
